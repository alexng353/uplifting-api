  "query": "\n        UPDATE users\n        SET \n            username = COALESCE($2, username),\n            real_name = COALESCE($3, real_name),\n            avatar_url = COALESCE($4, avatar_url)\n        WHERE id = $1\n        RETURNING id, username, real_name, email, avatar_url, email_verified, created_at\n        ",
