{
  "db_name": "PostgreSQL",
  "query": "\n        SELECT \n            f.id as friendship_id,\n            u.id as user_id,\n            u.username,\n            u.real_name,\n            u.avatar_url,\n            f.status,\n            f.created_at\n        FROM friendships f\n        JOIN users u ON u.id = f.user_id\n        WHERE f.friend_id = $1 AND f.status = 'pending'\n        ORDER BY f.created_at DESC\n        ",
